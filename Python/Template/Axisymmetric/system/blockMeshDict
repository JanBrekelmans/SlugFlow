{{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

scale   {scale};

vertices
(
    (0 0 0)				// 0
	({cX} 0 0)			// 1
	(0 {cYM} {cZ})		// 2
	({cX} {cyM} {cZ})	// 3
	({cX} {cY} {cZ})	// 4
	(0 {cY} {cZ})		// 5
);

blocks
(
    hex (0 1 1 0 2 3 4 5) (numX 1 numZ) simpleGrading (1.0 1.0 1.0)
);

edges
(
);

boundary
(
    axis
    {{
        type empty;
        faces
        (
            (0 1 1 0)
        );
    }}

    inlet
    {{
        type patch;
        faces
        (
            (0 0 5 2)
        );
    }}

    outlet
    {{
        type patch;
        faces
        (
            (1 1 4 3)
        );
    }}

    wall
	{{
		type wall;
		faces
		(
			(2 3 4 5)
		);
	}}

    front
    {{
        type wedge;
        faces
        (
			(0 1 3 2)
        );
    }}

    back
    {{
        type wedge;
        faces
        (
            (0 1 4 5)
        );
    }}
);

mergePatchPairs
(
